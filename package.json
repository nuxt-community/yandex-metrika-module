  "devDependencies": {
    "@nuxt/module-builder": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@release-it/conventional-changelog": "^4.3.0",
    "@types/node": "latest",
    "eslint": "latest",
