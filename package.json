{
  "name": "@nuxtjs/yandex-metrika",
  "version": "2.0.0",
  "description": "Add Yandex Metrika to your Nuxt application.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-community/yandex-metrika-module.git"
  },
  "homepage": "nuxt-community/yandex-metrika-module",
  "bugs": {
    "url": "https://github.com/nuxt-community/yandex-metrika-module/issues"
  },
